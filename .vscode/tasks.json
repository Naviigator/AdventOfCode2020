{
"version": "2.0.0",
"tasks": [
    {
        "label": "clean",
        "type": "shell",
        "command": "cargo",
        "args": ["clean"]
    },
    {
        "label": "build",
        "type": "shell",
        "command": "cargo",
        "args": ["build"],
        "group": {
            "kind": "build",
            "isDefault": true
        },
        "dependsOn": "clean"
    }
]
}